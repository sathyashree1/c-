#include<iostream>
using namespace std;
int main()
{
  string word, reversed_word;

    cout << "Enter a word: ";
    cin >> word;

    for (int i = word.length() - 1; i >= 0; i--) {
        reversed_word += word[i];
    }

    cout << "Reversed word: " << reversed_word ;

    return 0;
}

